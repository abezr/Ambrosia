{
  "name": "graphql-server",
  "version": "1.0.0",
  "description": "GraphQL server with Mongoose (MongoDB) and Node.js",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=* node --harmony src/server/index.js",
    "watch": "nodemon --exec npm run start",
    "client": "DEBUG=* babel-node script/query.js",
    "update-schema": "babel-node ./script/updateSchema",
    "seed": "babel-node script/seed"
  },
  "browserify": {
    "transform": [
      "reactify",
      "babelify"
    ]
  },
  "author": "RisingStack",
  "license": "ISC",
  "dependencies": {
    "co": "^4.5.4",
    "co-body": "^2.0.0",
    "debug": "^2.2.0",
    "graphql": "0.4.2",
    "koa": "^0.21.0",
    "koa-gql": "^0.1.2",
    "koa-mount": "^1.3.0",
    "koa-qs": "^2.0.0",
    "koa-router": "^5.1.1",
    "koa-serve": "^0.1.4",
    "mongoose": "^4.0.6",
    "react": "^0.14.0-beta3",
    "rethinkdb": "^2.0.2",
    "superagent": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-loader": "^5.3.2",
    "babel-relay-plugin": "^0.1.2",
    "babelify": "^6.1.3",
    "browserify": "^11.0.1",
    "chai": "^3.0.0",
    "co-mocha": "^1.1.2",
    "co-supertest": "0.0.10",
    "fs": "0.0.2",
    "graphql-relay": "^0.3.1",
    "koa-webpack-dev-middleware": "^1.0.1",
    "mocha": "^2.2.5",
    "sinon": "^1.15.4",
    "sinon-chai": "^2.8.0",
    "supertest": "^1.0.1",
    "webpack": "^1.11.0"
  }
}
